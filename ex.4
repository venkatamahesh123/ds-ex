import pandas as pd
import numpy as np

# Load the dataset from the GitHub URL
file_url = "https://raw.githubusercontent.com/venkatamahesh123/datasets/main/diabetes.csv"
df = pd.read_csv(file_url)

# Display the first few rows
print("First 5 rows of the dataset:")
print(df.head())

# Selecting numerical columns for analysis
numerical_cols = df.select_dtypes(include=[np.number]).columns

# Performing Univariate Analysis
print("\n--- Univariate Analysis ---\n")

for col in numerical_cols:
    print(f"\nAnalysis for '{col}':")
    print(f"Frequency Count:\n{df[col].value_counts().head()}")
    print(f"Mean: {df[col].mean():.2f}")
    print(f"Median: {df[col].median():.2f}")
    print(f"Mode: {df[col].mode().values}")
    print(f"Variance: {df[col].var():.2f}")
    print(f"Standard Deviation: {df[col].std():.2f}")
    print("-" * 50)


